name				MetaPress
baseurl				http://www.metapress.com/
submiturl			content/
submitparamsformat		k=author:"%s"
submitparamswithtitleformat	%s+AND+title:"%s"
pubpagelinkpattern		<h2>.*?<a href="/(content.*?/)"?>
startfield			o
authorspattern			Authors.*?</h2>(.*?)</ul>
titlepattern			<h1.*?>(.*?)</h1>
yearpattern			<div class="?online-date detail"?>[^<]*?([0-9]{4}).*?</div>
refpublistpattern		Referenced by.*?<div class="?sidebar-body"?>.*?<ol(.*?)</ol>
refpublistitempattern		<li>(.*?)</li>
refpubauthorspattern		>(.*?) \([0-9]{4}
refpubtitlepattern		\([0-9]{4}\)(.*?)\.
refpubyearpattern		\(([0-9]{4})\)