name				MetaPress
baseurl				http://www.metapress.com/
submiturl			content/
submitparamsformat		k=author:"%s"
submitparamswithtitleformat	%s+AND+title:"%s"
pubpagelinkpattern		<div class="listItemName">.*?href="/(content.*?/)\?.*?".*?</table>
startfield			o
authorspattern			Authors.*?</h5>.*?<div>(.*?)</div>
titlepattern			<h2.*?>(.*?)</h2>
yearpattern			<td class="labelName">(?:Copyright|Online date)</td>.*?([0-9]{4})
refpublistpattern		Referenced by.*?<div class="sidebarBody">.*?<ol(.*?)</ol>
refpublistitempattern		<li>(.*?)</li>
refpubauthorspattern		>(.*?) \([0-9]{4}
refpubtitlepattern		\([0-9]{4}\)(.*?)\.
refpubyearpattern		\(([0-9]{4})\)