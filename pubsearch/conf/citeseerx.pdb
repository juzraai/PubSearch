name				CiteSeerX
baseurl				http://citeseerx.ist.psu.edu/
submiturl			search
submitparamsformat		sort=cite&t=doc&q=author:"%s"
submitparamswithtitleformat	%s+AND+title:"%s"
pubpagelinkpattern		<h3>.*?href="/(viewdoc/summary.*?\?doi=.*?)".*?</h3>
bibtexpattern			<p>(@(.*?))</p>
refpublistpagelinkpattern	Citations:.*?href="/(.*?)"