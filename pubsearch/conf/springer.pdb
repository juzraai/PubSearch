name				SpringerLink
baseurl				http://www.springerlink.com/
submiturl			content/
submitparamsformat		k=au:"%s"
submitparamswithtitleformat	%s+ti:"%s"
pubpagelinkpattern		<li class="?primitive.*?<a href="/(content/.*?/)">
pubpagelinkmodformat		%sabout/
firstindex			1
startfield			o
authorspattern			<p class="?authors"?>(.*?)</p>
titlepattern			<div class="?text"?>.*?<h1.*?>(.*?)</h1>
