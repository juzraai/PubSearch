name				SpringerLink
baseurl				http://www.springerlink.com/
submiturl			content/
submitparamsformat		k=au:"%s"
submitparamswithtitleformat	%s+ti:"%s"
pubpagelinkpattern		<li class="primitive.*?<a href="/(content/.*?/)">
pubpagelinkmodformat		%sabout/
firstindex			1
startfield			o
authorspattern			<p class="authors">(.*?)</p>
titlepattern			<div class="text">.*?<h1.*?>(.*?)</h1>
yearpattern			(?:<div class="secondary">.*?, |<dt>Copyright Year.*?)([0-9]{4})
refpublistpagelinkpattern	<li><a href="/(content/.{16}/referrers/)"
refpublistblockpattern		<ol class="citations">(.*?)</ol>
refpubblockpattern		<li>(.*?)(?:<ul>.*?</ul>)?</li>
refpubauthorspattern		^(.*?)\(
refpubtitlepattern		\([0-9]{4}\)(.*?)\.
refpubyearpattern		\(([0-9]{4})\)